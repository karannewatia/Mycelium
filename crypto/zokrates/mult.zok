//import "hashes/mimc7/mimc7R90" as mimc

//Multiply together 10 ciphertexts (stored together in u and v) and return product
def main(private field[327680] u, private field[327680] v) ->  field[65536]:

  field[65536] result = [0; 65536]

  for u32 i in 0..32768 do
    field tmp_u = 1
    field tmp_v = 1
    for u32 j in 0..9 do
      tmp_u = tmp_u *  u[i+j*32768] 
      tmp_v = tmp_v *  v[i+j*32768] 
    endfor
    result[i] = tmp_u
    result[i+32768] = tmp_v
  endfor

  return result

